<div class="modal exe-modal-fade" tabindex="-1" id="modalReleaseNotes" role="dialog" aria-labelledby="modalReleaseNotesCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-confirm" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">{{ "Release notes" | trans }}</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="{{ "Close" | trans }}">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body help-modal help-modal-with-logo">
                <div class="help-modal-top">
                    <img src="style/workarea/images/exelearning.png" class="exe-logo content">
                </div>
                <div class="help-modal-content">
                    <h3 class="lead mb-4">{{ "Changelog" | trans }}</h3>
                    <div class="body-release">
                        <div class="changelog-content md-converted-content"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
