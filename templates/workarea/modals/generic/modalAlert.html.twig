<div class="modal exe-modal-fade" tabindex="-1" id="modalAlert" role="alert" aria-labelledby="modalAlertCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-alert" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title"></h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="{{ "Close" | trans }}">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body"></div>
            <div class="modal-footer">
                <button type="button" class="close btn btn-secondary" data-dismiss="modal">{{ "Close" | trans }}</button>
            </div>
        </div>
    </div>
</div>