<div class="modal exe-modal-fade" tabindex="-1" id="modalProperties" role="dialog" aria-labelledby="modalPropertiesCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-confirm" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">{{ "Properties" | trans }}</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="{{ "Close" | trans }}">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body form-properties"></div>
            <div class="modal-footer">
                <button type="button" class="confirm btn btn-primary">{{ "Save" | trans }}</button>
                <button type="button" class="close btn btn-secondary" data-dismiss="modal">{{ "Cancel" | trans }}</button>
            </div>
        </div>
    </div>
</div>
