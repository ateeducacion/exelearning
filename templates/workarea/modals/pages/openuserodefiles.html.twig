<div class="modal exe-modal-fade" tabindex="-1" id="modalOpenUserOdeFiles" role="dialog" aria-labelledby="modalOpenUserOdeFilesCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-confirm" role="document">
        <div class="modal-content" id="modalOpenUserOdeFilesContent">
            <div class="modal-header">
                <h5 class="modal-title">{{ "Open" | trans }}</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="{{ "Close" | trans }}">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="modal-body-content"></div>
            </div>
            <div class="modal-footer">
                <button type="button" class="confirm btn btn-primary">{{ "Open" | trans }}</button>
                <button type="button" class="close btn btn-secondary" data-dismiss="modal">{{ "Cancel" | trans }}</button>
            </div>
        </div>
    </div>
</div>